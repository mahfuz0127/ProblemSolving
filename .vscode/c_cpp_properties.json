{
  "code-runner.executorMap": {
  "cpp": "cd $dir && g++ $fileName -o output/$fileNameWithoutExt && output/$fileNameWithoutExt"
},

  "configurations": [
    {
      "name": "windows-gcc-x86",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "compilerPath": "C:/MinGW/bin/gcc.exe",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "windows-gcc-x86",
      "compilerArgs": [
        ""
      ]
    }
  ],
  "version": 4

}